{
  "name": "idevjs-koa2",
  "version": "1.0.0",
  "description": "iDevJS api server built on koa2",
  "main": "app.js",
  "scripts": {
    "test": "mocha --ui qunit --compilers js:babel-core/register",
    "debug": "npm run clean && node bin/debug",
    "watch": "nodemon --watch src src/babel.app.js",
    "dev": "babel src/ --watch --out-dir app/ --retain-lines --source-maps",
    "lint": "eslint src/",
    "compile": "babel src/ --out-dir app/ --retain-lines --source-maps",
    "clean": "rm -rf app/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idevjs/idevjs-koa2.git"
  },
  "keywords": [
    "idevjs",
    "koa2",
    "api",
    "rest"
  ],
  "author": "xuhong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idevjs/idevjs-koa2/issues"
  },
  "homepage": "https://github.com/idevjs/idevjs-koa2#readme",
  "dependencies": {
    "boom": "^4.2.0",
    "ioredis": "^2.4.3",
    "kcors": "^2.2.0",
    "koa": "^2.0.0-alpha.3",
    "koa-bodyparser": "^3.2.0",
    "koa-compose": "^3.1.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-jsonp": "^2.0.2",
    "koa-logger": "^2.0.0",
    "koa-mount": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.0.2",
    "lodash": "^4.16.4",
    "marked": "^0.3.6",
    "mongodb": "^2.2.10",
    "mongoose": "^4.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-watch": "^2.0.3",
    "chokidar": "^1.6.0",
    "colors": "^1.1.2",
    "debug": "^2.2.0",
    "eslint": "^3.1.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.0"
  }
}
